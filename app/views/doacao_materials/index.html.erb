<p id="notice"><%= notice %></p>

<h1>Listing Doacao Materials</h1>

<table>
  <thead>
    <tr>
      <th>Instuicao</th>
      <th>Quantidade</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doacao_materials.each do |doacao_material| %>
      <tr>
        <td><%= doacao_material.instuicao_id %></td>
        <td><%= doacao_material.quantidade %></td>
        <td><%= doacao_material.descricao %></td>
        <td><%= link_to 'Show', doacao_material %></td>
        <td><%= link_to 'Edit', edit_doacao_material_path(doacao_material) %></td>
        <td><%= link_to 'Destroy', doacao_material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doacao material', new_doacao_material_path %>
