                          <ul class="social-share">
                              <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                              <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                          </ul>
                          <div class="search">
                              <form role="form">
                                  <input type="text" class="search-form" autocomplete="off" placeholder="Pesquisar">
